    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es2022",
        "target": "es5",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "types": [
            "node", "webpack-env"
        ]
    }
}
