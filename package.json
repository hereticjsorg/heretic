    "imports": {
        "#root/*": "./*",
        "#core/*": "./src/core/*",
        "#components/*": "./src/core/components/*",
        "#build/*": "./src/build/*",
        "#lib/*": "./src/core/lib/*",
        "#site/*": "./site/*",
        "#etc/*": "./etc/*",
        "#src/*": "./src/*"
    },
    "scripts": {
        "server": "node dist/server",
        "configure": "node src/bin/configure",
        "setup": "node dist/setup",
