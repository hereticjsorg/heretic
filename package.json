        "cleanup-dev": "node src/bin/cleanup",
        "build": "node src/bin/build",
        "build-dev": "webpack --mode development",
        "build-production": "webpack --mode production",
        "test": "jest --runInBand",
        "doc": "documentation build src/core/*.ts -f md -o docs/core.md",
        "docker-compose": "node src/bin/docker-compose"
    },
    "keywords": [
        "heretic",
        "marko",
